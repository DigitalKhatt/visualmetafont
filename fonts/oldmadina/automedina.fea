    sub lam.medi by lam.medi.afterkaf;	  
  } kaflamalef.l1;	

	sub  [kaf.init kaf.medi]'lookup  kaflamalef.l1 [alef.fina lam.medi lam.fina lam.medi.laf]'lookup kaflamalef.l1;

} kaflamalef;



lookup otherliga {

